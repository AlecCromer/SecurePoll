<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ung.mccb.csci.csci3300.Controller.votingController">
    <children>
        <Button layoutX="504.0" layoutY="14.0" mnemonicParsing="false" onAction="#viewResults" text="View Results" />
        <Label fx:id="pollName" layoutX="263.0" layoutY="48.0" text="PollNameHere" />
        <Button layoutX="504.0" layoutY="361.0" mnemonicParsing="false" onAction="#toVoteTableView" text="Back to Polls" />
      <TableView fx:id="candidateList" layoutX="63.0" layoutY="114.0" prefHeight="224.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Label layoutX="101.0" layoutY="86.0" text="Choose your Candidate" />
      <Label layoutX="340.0" layoutY="106.0" text="CandidateNameHere" />
      <Button layoutX="355.0" layoutY="149.0" mnemonicParsing="false" onAction="#submitVote" text="Submit Vote" />
    </children>
</AnchorPane>
